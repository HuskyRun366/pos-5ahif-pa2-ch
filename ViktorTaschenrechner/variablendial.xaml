<Window x:Class="Russiantechnerrr.variablendial"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Russiantechnerrr"
        mc:Ignorable="d"
        Title="variablendial" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition>
            </RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            
        </Grid.ColumnDefinitions>
        <Label Content="Variable:" Grid.Row="0" Grid.Column="0"></Label>
        <Label Content="Value:" Grid.Row="1" Grid.Column="0"></Label>
        <TextBox Grid.Row="0" Grid.Column="1" x:Name="tbvar"></TextBox>
        <TextBox Grid.Row="1" Grid.Column="1" x:Name="tbval"></TextBox>
        <ListBox x:Name="lb" Grid.RowSpan="2" Grid.Row="2" Grid.Column="0"></ListBox>
        <Button x:Name="add" Content="Add" Grid.Row="2" Grid.Column="1" Click="add_Click"></Button>
        <Button x:Name="rmvar" Content="Remove" Grid.Row="3" Grid.Column="1" Click="rmvar_Click"></Button>
        <Button x:Name="close" Content="close" Grid.Row="4" Grid.ColumnSpan="2"></Button>



    </Grid>
</Window>
